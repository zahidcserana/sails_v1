<table>
    <thead>
    <tr>
        <th>ID
        </td>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user){ %>
        <tr>
            <td> <%= user.id %> </td>
            <td> <%= user.firstname %> </td>
            <td> <%= user.lastname %> </td>
        </tr>
    <% }) %>
    </tbody>
</table>
