<table>
    <thead>
    <tr>
        <th>ID
        </td>
        <th>Full Name</th>
        <th>Email</th>
        <th>Mobile</th>
    </tr>
    </thead>
    <tbody>
    <% users.forEach(function(user){ %>
        <tr>
            <td> <%= user.id %> </td>
            <td> <%= user.fullname %> </td>
            <td> <%= user.email %> </td>
            <td> <%= user.mobile %> </td>
        </tr>
    <% }) %>
    </tbody>
</table>
